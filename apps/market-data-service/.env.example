# Market Data Service Configuration

# Data Source: Mock, Binance, or Alpaca
MarketData__Source=Binance

# Symbols to track (comma-separated, no spaces in JSON array format)
MarketData__Symbols__0=BTCUSDT
MarketData__Symbols__1=ETHUSDT
MarketData__Symbols__2=SOLUSDT

# Binance Configuration
MarketData__Binance__WebSocketUrl=wss://stream.binance.com:9443
MarketData__Binance__ReconnectDelaySeconds=5
MarketData__Binance__UseTestnet=false

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.Hosting.Lifetime=Information
